{
    "routes": [

      {
        "route": "/visits",
        "allowedRoles": ["authenticated"]
      },

      {
        "route": "/about",
        "allowedRoles": ["authenticated"]
      },

      {
        "route": "/login",
        "serve": "/.auth/login/github"
      },

      {
        "route": "/logout",
        "serve": "/.auth/logout"
      },

      {
        "route": "/health-department",
        "allowedRoles": ["administrator"]
      }
    ],
    "defaultHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        "custom": "text/html"
    }
  }